// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Mae {  
  id        Int     @id @default(autoincrement())   
  nome       String
  endereco   String
  telefone   String
  dataNasc   DateTime
}

model Medico {
id        Int     @id @default(autoincrement())
CRM        String
nome       String
endereco   String
telefone   String
especialidade String

}

model Bebes {
  id        Int     @id @default(autoincrement())
nome  String
dataNasc DateTime
pesoNasc String
altura String
nomeMae String




}